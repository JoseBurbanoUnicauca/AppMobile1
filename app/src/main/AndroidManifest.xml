<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="edu.unicauca.tindu">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/logo_size"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_size"
        android:supportsRtl="true"
        android:theme="@style/Theme.Inicio_registro"
        tools:targetApi="31">
        <activity
            android:name="edu.unicauca.tindu.navegacion"
            android:exported="false" />
        <activity
            android:name="edu.unicauca.tindu.RegistrarseActivity"
            android:exported="false" />
        <activity
            android:name="edu.unicauca.tindu.IniciarSesionActivity"
            android:exported="false" />
        <activity
            android:name="edu.unicauca.tindu.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>